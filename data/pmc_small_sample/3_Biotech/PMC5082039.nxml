<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">3 Biotech</journal-id><journal-id journal-id-type="iso-abbrev">3 Biotech</journal-id><journal-title-group><journal-title>3 Biotech</journal-title></journal-title-group><issn pub-type="epub">2190-5738</issn><publisher><publisher-name>Springer Berlin Heidelberg</publisher-name><publisher-loc>Berlin/Heidelberg</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28330301</article-id><article-id pub-id-type="pmc">5082039</article-id><article-id pub-id-type="publisher-id">547</article-id><article-id pub-id-type="doi">10.1007/s13205-016-0547-y</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title>Evaluation of <italic>p</italic>-cresol degradation with polyphenol oxidase (PPO) immobilized in various matrices</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Edalli</surname><given-names>Vijayalakshmi A.</given-names></name><address><phone>+91-836-2215243</phone><email>vijuedalli@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Mulla</surname><given-names>Sikandar I.</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Eqani</surname><given-names>Syed Ali Musstjab Akber Shah</given-names></name><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Mahadevan</surname><given-names>Gurumurthy D.</given-names></name><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Sharma</surname><given-names>Rohit</given-names></name><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Shouche</surname><given-names>Yogesh</given-names></name><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author" corresp="yes"><name><surname>Kamanavalli</surname><given-names>Chandrappa M.</given-names></name><address><phone>+91-836-2215243</phone><email>cmkamanavalli@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><aff id="Aff1"><label>1</label>Department of Biochemistry, Karnatak University, Dharwad, Karnataka 580 003 India </aff><aff id="Aff2"><label>2</label>Key Laboratory of Urban Environment and Health, Institute of Urban Environment, Chinese Academy of Sciences, Xiamen, 361021 China </aff><aff id="Aff3"><label>3</label>Key Laboratory of Urban Pollutant Conversion, Institute of Urban Environment, Chinese Academy of Sciences, Xiamen, 361021 China </aff><aff id="Aff4"><label>4</label>National Center for Cell Science, University of Pune, Ganeshkhind, Pune, 411007 India </aff></contrib-group><pub-date pub-type="epub"><day>26</day><month>10</month><year>2016</year></pub-date><pub-date pub-type="pmc-release"><day>26</day><month>10</month><year>2016</year></pub-date><pub-date pub-type="ppub"><month>12</month><year>2016</year></pub-date><volume>6</volume><issue>2</issue><elocation-id>229</elocation-id><history><date date-type="received"><day>3</day><month>9</month><year>2016</year></date><date date-type="accepted"><day>12</day><month>10</month><year>2016</year></date></history><permissions><copyright-statement>&#x000a9; The Author(s) 2016</copyright-statement><license license-type="OpenAccess"><license-p>
