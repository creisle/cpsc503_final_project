<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">3 Biotech</journal-id><journal-id journal-id-type="iso-abbrev">3 Biotech</journal-id><journal-title-group><journal-title>3 Biotech</journal-title></journal-title-group><issn pub-type="ppub">2190-572X</issn><issn pub-type="epub">2190-5738</issn><publisher><publisher-name>Springer Berlin Heidelberg</publisher-name><publisher-loc>Berlin/Heidelberg</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28330081</article-id><article-id pub-id-type="pmc">4701709</article-id><article-id pub-id-type="publisher-id">322</article-id><article-id pub-id-type="doi">10.1007/s13205-015-0322-5</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title>Emerging CAM <italic>Ziziphus nummularia</italic> with in vivo sedative-hypnotic, antipyretic and analgesic attributes</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Rauf</surname><given-names>Abdur</given-names></name><address><email>mashaljcs@yahoo.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Ali</surname><given-names>Jawad</given-names></name><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Khan</surname><given-names>Haroon</given-names></name><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Mubarak</surname><given-names>Mohammad S.</given-names></name><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author" corresp="yes"><name><surname>Patel</surname><given-names>Seema</given-names></name><address><email>seemabiotech83@gmail.com</email></address><xref ref-type="aff" rid="Aff5">5</xref></contrib><aff id="Aff1"><label>1</label>Department of Geology, University of Swabi, Anbar, Khyber Pakhtunkhwa 23561 Pakistan </aff><aff id="Aff2"><label>2</label>Institute of Chemical Sciences, University of Peshawar, Peshawar, KP 25120 Pakistan </aff><aff id="Aff3"><label>3</label>Department of Pharmacy, Abdul Wali Khan University, Mardan, 23200 Pakistan </aff><aff id="Aff4"><label>4</label>Department of Chemistry, The University of Jordan, Amman, 11942 Jordan </aff><aff id="Aff5"><label>5</label>Bioinformatics and Medical Informatics Research Center, San Diego State University, San Diego, CA 92182 USA </aff></contrib-group><pub-date pub-type="epub"><day>5</day><month>1</month><year>2016</year></pub-date><pub-date pub-type="pmc-release"><day>5</day><month>1</month><year>2016</year></pub-date><pub-date pub-type="ppub"><month>6</month><year>2016</year></pub-date><volume>6</volume><issue>1</issue><elocation-id>11</elocation-id><history><date date-type="received"><day>22</day><month>2</month><year>2015</year></date><date date-type="accepted"><day>13</day><month>6</month><year>2015</year></date></history><permissions><copyright-statement>&#x000a9; The Author(s) 2015</copyright-statement><license license-type="OpenAccess"><license-p>
