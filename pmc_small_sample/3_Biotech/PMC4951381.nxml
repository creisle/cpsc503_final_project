ress><email>saleemwn806@gmail.com</email></address><xref ref-type="aff" rid="Aff1"/></contrib><contrib contrib-type="author"><name><surname>Gupta</surname><given-names>Raghbir Chand</given-names></name><address><email>guptarc53@rediffmail.com</email></address><xref ref-type="aff" rid="Aff1"/></contrib><aff id="Aff1">Department of Botany, Punjabi University, Patiala, Punjab 147002 India </aff></contrib-group><pub-date pub-type="epub"><day>19</day><month>7</month><year>2016</year></pub-date><pub-date pub-type="pmc-release"><day>19</day><month>7</month><year>2016</year></pub-date><pub-date pub-type="ppub"><month>12</month><year>2016</year></pub-date><volume>6</volume><issue>2</issue><elocation-id>156</elocation-id><history><date date-type="received"><day>13</day><month>1</month><year>2016</year></date><date date-type="accepted"><day>4</day><month>7</month><year>2016</year></date></history><permissions><copyright-statement>&#x000a9; The Author(s) 2016</copyright-statement><license license-type="OpenAccess"><license-p>
