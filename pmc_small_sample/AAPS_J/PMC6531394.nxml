				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$ \frac{dC1\ }{dt}=- kel\times C1-k12\times C1+k21\times C2\times \frac{V2}{V1}- ko{n}_{CD3}\times C1\times \left( TotCD{3}_p- DCD{3}_p\ \right)+ kof{f}_{CD3}\times DCD{3}_p- ko{n}_{Pcad}\times C1\times sPcad+ kof{f}_{Pcad}\times DPca{d}_p-\mathrm{Tumor}\ \mathrm{Disposition}\times \frac{TV}{V1};\mathrm{C}1\left(\mathrm{t}=0\right)=\mathrm{Dose}\ \mathrm{in}\ \mathrm{nmols} $$\end{document}</tex-math><mml:math id="M48" display="block"><mml:mfrac><mml:mrow><mml:mi mathvariant="italic">dC</mml:mi><mml:mn>1</mml:mn><mml:mspace width="0.25em"/></mml:mrow><mml:mi mathvariant="italic">dt</mml:mi></mml:mfrac><mml:mo>=</mml:mo><mml:mo>&#x02212;</mml:mo><mml:mi mathvariant="italic">kel</mml:mi><mml:mo>&#x000d7;</mml:mo><mml:mi>C</mml:mi><mml:mn>1</mml:mn><mml:mo>&#x02212;</mml:mo><mml:mi>k</mml:mi><mml:mn>12</mml:mn><mml:mo>&#x000d7;</mml:mo><mml:mi>C</mml:mi><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:mi>k</mml:mi><mml:mn>21</mml:mn><mml:mo>&#x000d7;</mml:mo><mml:mi>C</mml:mi><mml:mn>2</mml:mn><mml:mo>&#x000d7;</mml:mo><mml:mfrac><mml:mrow><mml:mi>V</mml:mi><mml:mn>2</mml:mn></mml:mrow><mml:mrow><mml:mi>V</mml:mi><mml:mn>1</mml:mn></mml:mrow></mml:mfrac><mml:mo>&#x02212;</mml:mo><mml:mi mathvariant="italic">ko</mml:mi><mml:msub><mml:mi>n</mml:mi><mml:mrow><mml:mi mathvariant="italic">CD</mml:mi><mml:mn>3</mml:mn></mml:mrow></mml:msub><mml:mo>&#x000d7;</mml:mo><mml:mi>C</mml:mi><mml:mn>1</mml:mn><mml:mo>&#x000d7;</mml:mo><mml:mfenced close=")" open="("><mml:mrow><mml:mtext mathvariant="italic">TotCD</mml:mtext><mml:msub><mml:mn>3</mml:mn><mml:mi>p</mml:mi></mml:msub><mml:mo>&#x02212;</mml:mo><mml:mi mathvariant="italic">DCD</mml:mi><mml:msub><mml:mn>3</mml:mn><mml:mi>p</mml:mi></mml:msub><mml:mspace width="0.25em"/></mml:mrow></mml:mfenced><mml:mo>+</mml:mo><mml:mi mathvariant="italic">kof</mml:mi><mml:msub><mml:mi>f</mml:mi><mml:mrow><mml:mi mathvariant="italic">CD</mml:mi><mml:mn>3</mml:mn></mml:mrow></mml:msub><mml:mo>&#x000d7;</mml:mo><mml:mi mathvariant="italic">DCD</mml:mi><mml:msub><mml:mn>3</mml:mn><mml:mi>p</mml:mi></mml:msub><mml:mo>&#x02212;</mml:mo><mml:mi mathvariant="italic">ko</mml:mi><mml:msub><mml:mi>n</mml:mi><mml:mtext mathvariant="italic">Pcad</mml:mtext></mml:msub><mml:mo>&#x000d7;</mml:mo><mml:mi>C</mml:mi><mml:mn>1</mml:mn><mml:mo>&#x000d7;</mml:mo><mml:mtext mathvariant="italic">sPcad</mml:mtext><mml:mo>+</mml:mo><mml:mi mathvariant="italic">kof</mml:mi><mml:msub><mml:mi>f</mml:mi><mml:mtext mathvariant="italic">Pcad</mml:mtext></mml:msub><mml:mo>&#x000d7;</mml:mo><mml:mtext mathvariant="italic">DPca</mml:mtext><mml:msub><mml:mi>d</mml:mi><mml:mi>p</mml:mi></mml:msub><mml:mo>&#x02212;</mml:mo><mml:mtext>Tumor Disposition</mml:mtext><mml:mo>&#x000d7;</mml:mo><mml:mfrac><mml:mi mathvariant="italic">TV</mml:mi><mml:mrow><mml:mi>V</mml:mi><mml:mn>1</mml:mn></mml:mrow></mml:mfrac><mml:mo>;</mml:mo><mml:mi mathvariant="normal">C</mml:mi><mml:mn>1</mml:mn><mml:mfenced close=")" open="("><mml:mrow><mml:mi mathvariant="normal">t</mml:mi><mml:mo>=</mml:mo><mml:mn>0</mml:mn></mml:mrow></mml:mfenced><mml:mo>=</mml:mo><mml:mtext>Dose in</mml:mtext><mml:mspace width="0.25em"/><mml:mtext>nmols</mml:mtext></mml:math><graphic xlink:href="12248_2019_332_Article_Equ24.gif" position="anchor"/></alternatives></disp-formula><disp-formula id="Equ25"><label>25</label><alternatives><tex-math id="M49">\documentclass[12pt]{minimal}
